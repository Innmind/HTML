{
    "name": "innmind/html",
    "type": "library",
    "description": "Library to abstract html manipulation",
    "keywords": ["html"],
    "homepage": "http://github.com/Innmind/Html",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Html/issues"
    },
    "require": {
        "php": "~7.4",
        "innmind/xml": "~5.0",
        "symfony/dom-crawler": "^4.2|^5.0",
        "innmind/url": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Html\\": "src/"
        },
        "files": ["src/bootstrap.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Html\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0"
    }
}
